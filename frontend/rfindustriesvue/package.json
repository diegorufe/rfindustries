{
  "name": "rfindustriesvue",
  "version": "0.0.1b",
  "private": "true",
  "main": "./src/lib/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "fmt-app": "prettier --write --ignore-unknown ./src",
    "fmt-chk": "prettier --check --ignore-unknown ./src",
    "clean": "del-cli --force ./dist",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "dependencies": {
    "rfindustriescore": "file:../rfindustriescore",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.11",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.25.5",
    "@vitest/ui": "^0.25.5",
    "@vue/test-utils": "^2.0.0",
    "del-cli": "^5.0.0",
    "jest": "^29.3.1",
    "jsdom": "^20.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.5",
    "vue-tsc": "^1.0.9"
  }
}