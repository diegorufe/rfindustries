  "devDependencies": {
    "@types/jest": "^29.2.3",
    "del-cli": "^5.0.0",
    "jest": "^29.3.1",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.6.4"
  }
}
